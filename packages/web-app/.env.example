# 曲マスタ取得元: web | mock | custom
VITE_SONG_MASTER_SOURCE=web

# webモード: GitHub Releases の tag URL
VITE_SONG_MASTER_WEB_RELEASE_TAG_URL=https://github.com/tts1374/iidx_all_songs_master/releases/tag/latest
# webモード開発時: Vite proxyのベースURL（通常は変更不要）
# VITE_SONG_MASTER_WEB_DEV_PROXY_BASE_URL=/__song-master-proxy__

# mockモード: scripts/mock-song-master-server.mjs の公開先
VITE_SONG_MASTER_MOCK_BASE_URL=http://localhost:8787/song-master

# customモード用（または強制上書き用）
# VITE_SONG_MASTER_LATEST_URL=
# VITE_SONG_MASTER_BASE_URL=

# アプリが受け入れる曲マスタschema_version
VITE_SONG_MASTER_SCHEMA_VERSION=33
